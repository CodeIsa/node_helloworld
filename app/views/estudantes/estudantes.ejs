<!DOCTYPE html>
<html lang="pt-br">
<head>
	<meta charset="utf-8" />
	<title>Estudantes</title>
</head>
<body>
	<h1>Estudantes</h1>

	<section>
		<% if(erros) { %>
		   <ul>
			   <%for(let i = 0; i < erros.length; i++){%>
				   <li>
					   <%= erros[i].msg %>
				   </li>
			   <% } %>
		   </ul>
		<% } %>
	</section>
	<table>
		<tr>
			<th>Nome</th>
			<th>Nota</th>
		</tr>
		<% for(let i=0; i<estudantes.length; i++) {%>
		<tr>
			<td><%= estudantes[i].nome %></td>
			<td><%= estudantes[i].nota %></td>
		</tr>
		<% } %>
	</table>
</body>
</html>